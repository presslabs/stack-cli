{
    "name": "presslabs-stack/cli",
    "description": "",
    "type": "wp-cli-package",
    "homepage": "https://github.com/presslabs-stack/cli",
    "license": "MIT",
    "authors": [],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Stack\\": "src/"
        },
        "files": [ "command.php" ]
    },
    "require-dev": {
        "wp-cli/wp-cli": "^2.1.0",
        "phpunit/phpunit": "^4.8",
        "behat/behat": "^2.5",
        "squizlabs/php_codesniffer": "^3.0.2",
        "dealerdirect/phpcodesniffer-composer-installer": "~0.4.4",
        "wp-coding-standards/wpcs": "~1.0.0",
        "wp-cli/wp-cli-bundle": "^2.2"
    },
    "scripts": {
        "lint": "phpcs",
       "test": "phpunit",
       "run-behat": "behat --ansi"
    }
}
